{ 
  "defaultSeverity":"error",
  "extends":[ 
     "tslint:recommended"
  ],
  "jsRules":{ 

  },
  "rules":{ 
     "quotemark":[ 
        true,
        "single",
        "jsx-double"
     ],
     "semicolon":[ 
        true,
        "never"
     ],
     "indent":[ 
        true,
        "tabs",
        2
     ],
     "array-type":false,
     "object-literal-sort-keys":false,
     "ordered-imports":false,
     "no-console":false
  },
  "rulesDirectory":[ 

  ]
}